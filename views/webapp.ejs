<div>
	<div style="text-align: right;">message notification placeholder<img class="" src="messages.png" style="height:2rem;"></div>

	<header class="card card-body">
		<div class="grid-container">
			<div class="item1">NewFeatureItem</div>
			<div class="item2"><p class="nav-link"><span id="client-username"><%= user.name %></span></p></div>  
			<div class="item3"><p class="nav-link"><span id="client-room"><%= user.last_room %></span></p></div>
		</div>
	</header>
	<main>
		<div class="chat-messages" style="overflow-y:auto; width: auto; height:215px; padding: 20px; border: solid 1px #EEE;">
	</main>
	<div>

	</div>
	<footer>
		<div class="grid-container1">
			<div class="item4">NewFeature1Item</div>
			<div class="item5">NewFeature2Item</div>  
			<div class="item6">		
				<form class="form-group" id="chat-form">
				<input
					class="form-control"
					id="msg"
					type="text"
					placeholder="Enter Message"
					required
					autocomplete="off"
				/>
				<button class="btn btn-primary btn-block mb-2"><i class="fas fa-paper-plane"></i> Send</button>
				</form>
			</div>
		</div>
	</footer>

</div>
	<script src="/socket.io/socket.io.js"></script>
	<script src="/DOM/dom.js"></script>
	<script src="/DOM/utils.js"></script>
	<script src="/client.js"></script>
	<link href="/css/style.css" rel="stylesheet" type="text/css">